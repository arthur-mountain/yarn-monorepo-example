{
  "name": "@yarn-monorepo-example/components",
  "type": "module",
  "version": "1.0.1",
  "license": "ISC",
  "types": "dist/types/index.d.ts",
  "module": "dist/esm/index.js",
  "browser": "dist/esm/index.js",
  "main": "dist/umd/index.js",
  "unpkg": "dist/umd/index.js",
  "umd:main": "dist/umd/index.js",
  "sideEffects": false,
  "scripts": {
    "build:types": "tsc",
    "build:esm": "webpack --env target=esm --config ./build/webpack.config.ts",
    "build:cjs": "webpack --env target=cjs --config ./build/webpack.config.ts",
    "build:umd": "webpack --env target=umd --config ./build/webpack.config.ts",
    "build": "webpack --mode production --config ./build/webpack.config.ts && yarn run build:types"
  },
  "dependencies": {
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/react": "^18.3.2",
    "@types/react-dom": "^18.3.0",
    "@babel/preset-react": "^7.24.1",
    "metro-react-native-babel-preset": "^0.77.0"
  }
}
